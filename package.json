{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "React & Redux boilerplate for awesome single-page apps",
  "main": "index.js",
  "scripts": {
    "clean": "find www ! -name '.gitkeep' -type f -exec rm -f {} +",
    "slate": "rm -rf node_modules && npm install",
    "build": "npm-run-all clean build:client build:server",
    "build:client": "webpack --config config/webpack.client.js",
    "build:server": "webpack --config config/webpack.server.js",
    "watch:client": "webpack --watch --config config/webpack.client.js",
    "watch:server": "webpack --watch --config config/webpack.server.js",
    "watch:bundle": "nodemon www/server.bundle.js",
    "start": "npm-run-all clean start:dev",
    "start:dev": "babel-node src/server/express.dev.js",
    "start:server": "NODE_ENV=development node www/server.bundle.js",
    "start:watch": "npm-run-all build -p watch:client watch:server watch:bundle",
    "lint": "eslint --ext .js src/",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thisiskylierose/react-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "boilerplate"
  ],
  "author": "Kylie Rose",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thisiskylierose/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/thisiskylierose/react-redux-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/node": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.2.1",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-react": "7.0.0",
    "autoprefixer": "9.4.2",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "css-loader": "2.0.1",
    "cssnano": "4.1.7",
    "ejs": "2.6.1",
    "eslint": "5.10.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "isomorphic-style-loader": "4.0.0",
    "mini-css-extract-plugin": "0.5.0",
    "nodemon": "1.18.9",
    "npm-run-all": "4.1.5",
    "postcss-loader": "3.0.0",
    "prettier": "1.15.3",
    "react-hot-loader": "4.6.0",
    "redux-logger": "3.0.6",
    "style-loader": "0.23.1",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.3"
  },
  "dependencies": {
    "@babel/runtime": "7.2.0",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-redux": "6.0.0",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0"
  }
}
